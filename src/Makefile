# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=run
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),run)
  Daedalus_Core_config = run
endif
ifeq ($(config),static-build)
  Daedalus_Core_config = static-build
endif
ifeq ($(config),dynamic-build)
  Daedalus_Core_config = dynamic-build
endif

PROJECTS := Daedalus-Core

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

Daedalus-Core:
ifneq (,$(Daedalus_Core_config))
	@echo "==== Building Daedalus-Core ($(Daedalus_Core_config)) ===="
	@${MAKE} --no-print-directory -C build/daedalus-core -f Makefile config=$(Daedalus_Core_config)
endif

clean:
	@${MAKE} --no-print-directory -C build/daedalus-core -f Makefile clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  run"
	@echo "  static-build"
	@echo "  dynamic-build"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   Daedalus-Core"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"